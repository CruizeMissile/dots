
# This is the main entry point for git's config file.
# The config has been broken up into different files for sanity.
# We are including all of the files that the config was broken up info.

[include]
    path = ./etc/alias/bootstrap
    path = ./etc/alias/branch
    path = ./etc/alias/commit
    path = ./etc/alias/logs
    path = ./etc/alias/rebase
    path = ./etc/alias/shorthand
    path = ./etc/alias/status
    path = ./etc/alias/submodule
    path = ./etc/alias/updates
    path = ./etc/alias/utils
    path = ./etc/core
    path = ./etc/url
    path = ~/.local/etc/git/config

